{
    "compilerOptions": {
        "outDir": "dist",
        "noImplicitAny": false,
        "target": "esnext",
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "allowJs": true,
        "noUnusedLocals": false,
        "strictNullChecks": true,
    },
    "include": [
        "rollup.config.ts",
    ],
    "exclude": [
        "node_modules/**"
    ]
}